{
  "name": "umi-h5-boilerplate",
  "version": "0.0.0",
  "scripts": {
    "start": "umi dev",
    "dev": "umi dev",
    "build": "umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "antd-mobile": "^5.4.0",
    "react": "^16",
    "react-dom": "^16"
  },
  "devDependencies": {
    "@types/react": "^16",
    "@types/react-dom": "^16",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "umi": "^3",
    "umi-preset-mobile": "^1"
  }
}
